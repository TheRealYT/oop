<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER_RIGHT" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.edu.admission_system.ui.AdminController">
   <children>
      <HBox alignment="CENTER_RIGHT">
         <children>
            <HBox alignment="CENTER_LEFT" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Universities" />
               </children>
            </HBox>
            <Button mnemonicParsing="false" onAction="#add" text="+ Add" />
            <Button mnemonicParsing="false" onAction="#delete" text="Delete">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin></Button>
            <Button layoutX="197.0" layoutY="20.0" mnemonicParsing="false" onAction="#initialize" text="Refresh">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <TableView fx:id="table" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="colId" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="colAbbr" prefWidth="75.0" text="Abbr" />
            <TableColumn fx:id="colName" prefWidth="75.0" text="Name" />
        </columns>
      </TableView>
   </children>
</VBox>
