<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_RIGHT" spacing="10.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.edu.admission_system.ui.DepartmentController">
   <children>
      <ComboBox fx:id="selectDept" prefHeight="25.0" prefWidth="184.0" promptText="Select Department" />
      <TextField fx:id="txtSlots" promptText="Available Slots" />
      <Button fx:id="btnAdd" mnemonicParsing="false" onAction="#addDept" text="Add" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
